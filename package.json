{
  "name": "serverless-boilerplate",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "debug": "SLS_DEBUG=* sls offline start",
    "commit": "cz",
    "lint": "./node_modules/.bin/eslint \"./src/**/*.{ts, js, json}\"  --fix --config \"./.eslintrc.js\"",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf \"node_modules\" && rm -rf \".esbuild\" && rm -rf \".serverless\" && rm -rf \"*lock\" && yarn"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "axios": "^0.24.0",
    "colors": "^1.4.0",
    "luxon": "^2.4.0",
    "pino": "^7.11.0",
    "pino-elasticsearch": "^6.2.0",
    "pino-http": "^6.6.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^7.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/luxon": "^2.3.2",
    "@types/node": "^14.14.25",
    "@types/pino-multi-stream": "^5.1.3",
    "esbuild": "^0.14.11",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^9.2.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "ts-node": "^10.0.0",
  "tsconfig-paths": "^3.10.1"
}
