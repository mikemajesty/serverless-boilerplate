{
  "name": "serverless-boilerplate",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "debug": "SLS_DEBUG=* sls offline start",
    "commit": "cz",
    "serverless:install": "yarn clean && yarn install",
    "lint": "./node_modules/.bin/eslint \"./src/**/*.{ts, js, json}\" --config \"./.eslintrc.js\" --fix",
    "prepare": "husky install && chmod ug+x .husky/pre-commit && .husky/commit-msg",
    "clean": "rm -rf \"node_modules\" && rm -rf \".esbuild\" && rm -rf \".serverless\" && rm -rf \"*lock\"",
    "test": "jest --detectOpenHandles --maxWorkers=50%",
    "test:coverage": "jest --detectOpenHandles --coverage"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "axios": "^0.24.0",
    "colors": "^1.4.0",
    "luxon": "^2.4.0",
    "pino": "^7.11.0",
    "pino-elasticsearch": "^6.2.0",
    "pino-http": "^6.6.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^7.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^27.5.2",
    "@types/luxon": "^2.3.2",
    "@types/node": "^14.14.25",
    "@types/pino-multi-stream": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.3.2",
    "serverless": "^3.0.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^9.2.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "cz-conventional-changelog": "^3.0.1"
  },
  "author": {
    "name": "Mike Lima",
    "email": "mike.rodrigues.lima@gmail.com"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}